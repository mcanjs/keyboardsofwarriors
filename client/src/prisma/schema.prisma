generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Matches {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model User {
  id               String  @id @default(auto()) @map("_id") @db.ObjectId
  email            String  @unique
  username         String  @unique
  isUserInApproval Boolean
  lose             BigInt
  matchPosition    String
  password         String
  queueBan         String
  queuePosition    String
  rank             BigInt
  win              BigInt
}
