generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Matches {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model User {
  id               String  @id @default(auto()) @map("_id") @db.ObjectId
  email            String  @unique
  lose             BigInt
  password         String
  queueBan         String
  rank             BigInt
  username         String  @unique
  win              BigInt
}
