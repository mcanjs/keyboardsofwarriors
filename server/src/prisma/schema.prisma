model User {
  id            String @id @default(auto()) @map("_id") @db.ObjectId
  email         String @unique
  username      String @unique
  win           Int
  lose          Int
  rank          Int
  password      String
  queueTierIndex Int
  competitiveTierIndex Int
  isUserInApproval Boolean
  queueBan String
}
